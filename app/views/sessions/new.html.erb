<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: "black_letter" %>
  <%= f.label :password %>
  <%= f.password_field :password, class: "black_letter" %>
  <%= f.submit "Log in", class: "black_letter" %>
<% end %>
